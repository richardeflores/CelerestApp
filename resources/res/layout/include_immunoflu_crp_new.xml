<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:orientation="vertical" android:background="#ffffff" android:layout_width="match_parent" android:layout_height="match_parent">
    <TextView android:gravity="center" android:background="#fae8c8" android:padding="5dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/urinalysis_tip"/>
    <LinearLayout android:background="#ebebeb" android:layout_width="match_parent" android:layout_height="wrap_content">
        <TextView android:text="@string/project" style="@style/operate_result_text"/>
        <TextView android:text="@string/result" android:layout_weight="1" style="@style/operate_result_text"/>
        <TextView android:text="@string/unit" android:layout_weight="1" style="@style/operate_result_text"/>
        <TextView android:text="@string/reference_range" style="@style/operate_result_text"/>
    </LinearLayout>
    <ScrollView android:id="@+id/svAnalyzeResult" android:scrollbars="none" android:layout_width="match_parent" android:layout_height="match_parent">
        <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
            <LinearLayout android:gravity="center" android:id="@+id/ll_one" android:layout_width="match_parent" android:layout_height="wrap_content">
                <TextView android:id="@+id/tv_name_one" android:text="@string/immuoflu_crp" style="@style/operate_result_text"/>
                <TextView android:textColor="@color/analyzeResultValue" android:id="@+id/tv_item_one" android:layout_weight="1" style="@style/operate_result_text"/>
                <TextView android:id="@+id/tv_unit_one" android:text="@string/immuoflu_hscrp_unit" android:layout_weight="1" style="@style/operate_result_text"/>
                <TextView android:id="@+id/tv_range_one" android:text="@string/immuoflu_saa_range" style="@style/operate_result_text"/>
            </LinearLayout>
            <LinearLayout android:gravity="center" android:id="@+id/ll_two" android:layout_width="match_parent" android:layout_height="wrap_content">
                <TextView android:id="@+id/tv_name_two" android:text="@string/immuoflu_hscrp" style="@style/operate_result_text"/>
                <TextView android:textColor="@color/analyzeResultValue" android:id="@+id/tv_item_two" android:layout_weight="1" style="@style/operate_result_text"/>
                <TextView android:id="@+id/tv_unit_two" android:text="@string/immuoflu_hscrp_unit" android:layout_weight="1" style="@style/operate_result_text"/>
                <TextView android:id="@+id/tv_range_two" android:text="@string/immuoflu_hscrp_range" style="@style/operate_result_text"/>
            </LinearLayout>
            <LinearLayout android:gravity="center" android:id="@+id/ll_three" android:visibility="invisible" android:layout_width="match_parent" android:layout_height="wrap_content">
                <TextView android:id="@+id/tv_name_three" style="@style/operate_result_text"/>
                <TextView android:textColor="@color/analyzeResultValue" android:id="@+id/tv_item_three" android:layout_weight="1" style="@style/operate_result_text"/>
                <TextView android:id="@+id/tv_unit_three" android:text="@string/immuoflu_hscrp_unit" android:layout_weight="1" style="@style/operate_result_text"/>
                <TextView android:id="@+id/tv_range_three" android:text="@string/immuoflu_hscrp_range" style="@style/operate_result_text"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/ll_four" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="40dp">
                <TextView android:gravity="center" android:layout_width="wrap_content" android:layout_height="40dp" android:text="@string/immuoflu_item_name_reagent" android:layout_marginStart="40dp" android:layout_marginEnd="10dp"/>
                <TextView android:gravity="center_vertical" android:id="@+id/tv_reagent" android:layout_width="match_parent" android:layout_height="40dp" android:layout_marginEnd="10dp"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/ll_five" android:layout_width="match_parent" android:layout_height="wrap_content">
                <TextView android:gravity="center" android:layout_width="wrap_content" android:layout_height="40dp" android:text="@string/immuoflu_measure_time" android:layout_marginStart="40dp" android:layout_marginEnd="10dp"/>
                <TextView android:gravity="center_vertical" android:id="@+id/tv_measure_time" android:layout_width="match_parent" android:layout_height="40dp" android:layout_marginEnd="10dp"/>
            </LinearLayout>
            <LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content"/>
        </LinearLayout>
    </ScrollView>
</LinearLayout>
